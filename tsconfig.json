{
    "include": [
        "./src/**/*"
    ],
    "compilerOptions": {
        "strict": true,
        "esModuleInterop": true,
        "lib": [
            "dom",
            "es2015",
            "ESNext"
        ],
        
        "target": "ES5",
        "module": "es2020",
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "downlevelIteration": true,
        "baseUrl": "src",
        "paths": {
            "assets/*": ["assets/*"],
            "textures/*": ["assets/textures/*"],
            "materials/*": ["materials/*"],
            "models/*": ["assets/models/*"],
            "objects/*": ["components/objects/*"]
          }
        
    }
}
